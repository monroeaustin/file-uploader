<section class="flex max-w-6xl py-6 gap-20 mx-auto mb-60" id="contact">

          <div class="h-140">
            <%- include ('messagesvg') %>

          </div>
          <div class="flex justify-center flex-col">
            <div class="flex flex-col gap-2 mb-4">
              <h1 class="text-2xl font-semibold"> Have a question?</h1>
              <p class="text-slate-400">Feel free to contact us.</p>
            </div>
            <form action="" class="flex-col flex max-w-lg ">
              <div class="flex flex-col gap-2"> <label for="email">Email</label>
                <input type="email" placeholder="Your Email" class="border rounded-md py-2 mb-4 px-2">
              </div> 
              <div class="flex flex-col gap-2"><label for="Questions">Questions</label>
                <textarea name="Questions" id="questions" rows="8" cols="40"
                  class="border rounded-md py-1 resize-none px-2" placeholder="Let us know how we can help!"></textarea>
              </div>

              <button class="text-white bg-[#6A4BFF] rounded-md py-2 mt-4"> Submit</button>
            </form>
          </div>
        </section>